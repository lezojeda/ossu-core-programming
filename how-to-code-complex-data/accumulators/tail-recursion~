#reader(lib "htdp-intermediate-lambda-reader.ss" "lang")((modname tail-recursion) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define (sum lon0)
  ;; acc√ë
  (local [(define (sum lon acc)
            (cond [(empty? lon) (... acc)]]
                  [else
                   ; (... acc
                   ; (first lon)
                        (sum (rest lon)))]))]
    (sum lon0 ...)))